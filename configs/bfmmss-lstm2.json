{
    "name": "bfmmss-lstm",
    "max_sentence_size": 150,
    "network": {
        "emb_enc_dim": 128,
        "enc_dropout": 0.1,
        "enc_hid_dim": 128,
        "enc_n_layers": 2
    },
    "model": "linear-lstm",
    "batch_size": 32,
    "learner": {
        "lr_grace_periode": 2,
        "lr_patience": 2,
        "lr": 0.0005,
        "lr_factor": 0.5
    },
    "label_encoder": {
        "normalize": false,
        "lower": false
    },
    "datasets": {
        "test": "./datasets/fro/test.tsv",
        "train": "./datasets/fro/train.tsv",
        "dev": "./datasets/fro/dev.tsv",
        "random": false
    }
}
