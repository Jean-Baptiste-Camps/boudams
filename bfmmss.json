{
    "name": "bfmmss",
    "max_sentence_size": 150,
    "network": {
        "emb_enc_dim": 256,
        "enc_n_layers": 10,
        "enc_kernel_size": 5,
        "enc_dropout": 0.25
    },
    "model": "linear-conv",
    "batch_size": 32,
    "learner": {
        "lr_grace_periode": 2,
        "lr_patience": 2,
        "lr": 0.0001
    },
    "label_encoder": {
        "normalize": true,
        "lower": true
    },
    "datasets": {
        "test": "./test_data/bfmmss/test.tsv",
        "train": "./test_data/bfmmss/train.tsv",
        "dev": "./test_data/bfmmss/dev.tsv",
        "random": true
    }
}